@using MudBlazor
<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            <MudIcon Icon="@Icons.Material.Filled.Update" Class="mr-3 mb-n1" />
            Cropped canvas data
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudImage Src="@Src" ObjectFit="ObjectFit.Contain" Class="w-100">

        </MudImage>
    </DialogContent>
    <DialogActions>

    </DialogActions>
</MudDialog>
@code {

    [CascadingParameter] private MudDialogInstance MudDialog { get; set; }

    [Parameter]
    [System.ComponentModel.DataAnnotations.Required]
    public string Src { get; set; }
}